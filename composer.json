        "contao/core-bundle":"^3.5.9 || ~4.1",
	"contao-community-alliance/composer-plugin":"^2.4.1 || ~3.0"
    },
    "autoload":{
        "psr-0": {
            "Asc\\": [
                "system/modules/simple_inventory_tracker/library"
