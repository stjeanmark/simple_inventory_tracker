    "homepage": "http://markstjean.com/",
    "keywords":["contao", "simple", "inventory", "tracker", "Mark St. Jean"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Mark St. Jean",
            "homepage":"http://markstjean.com/"
        }
    ],
    "require":{
